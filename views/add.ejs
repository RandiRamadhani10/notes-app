<%- include('partials/doctype'); %>
<body>
    <%- include('partials/navbar'); %>
    <div class="container"> 
        <form method="POST">
            <div class="form-group">
              <label for="note-id">Title</label>
              <input type="text" class="form-control" id="note-id" name="title" required>
            </div>
            <div class="form-group">
                <label for="note-body">Body</label>
                <input type="text" class="form-control" id="note-body" rows="3" name="body" required></input>
            </div>
            <button type="submit" class="btn btn-info">Submit</button>
          </form>
          <%- include('partials/alert-error'); %>
    </div>
    <%- include('partials/script'); %>
</body>
<%- include('partials/doctype-end'); %>
